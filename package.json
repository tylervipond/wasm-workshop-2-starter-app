{
  "name": "wasm-workshop-2-starter-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "as-bind": "^0.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "use-as-bind": "^0.1.0"
  },
  "scripts": {
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:asc": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b public/main.wasm --optimize",
    "build:asc-debug": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b public/main-debug.wasm -t public/main-debug.wat --sourceMap --debug",
    "start:asc": "onchange -i 'assembly/**/*' -- npm run build:asc-debug",
    "build": "npm run build:asc && npm run build:react",
    "start": "concurrently --kill-others \"npm:start:asc\" \"npm:start:react\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "assemblyscript": "^0.12.3",
    "concurrently": "^5.2.0",
    "onchange": "^7.0.2"
  }
}
